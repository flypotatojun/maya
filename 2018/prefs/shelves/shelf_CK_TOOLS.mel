insertText (\"\\n>>  \"+$filePath+\"   Linked\") tjh_lost_textures_finder_findedScrollField;\t\t\n\t\t}\n\t}\n\n\t//output results ===================================\n\t\n\t//path count\n\tstring $lostPathes[];\n\tstring $linkedPather[];\n\tint $lostPathes_file_count[];\n\tint $linkedPather_file_count[];\n\t\n\n\n\tscrollField -e  -insertionPosition 0 -insertText (\"\\n>> \\n =======  \"+size($tjh_lost_textures_finder__LinkedResultList)+\" files Linked ======\") tjh_lost_textures_finder_findedScrollField;\n\tscrollField -e -insertionPosition 0 -insertText (\"\\n>> \\n =======  \"+size($tjh_lost_textures_finder__LostResultList)+\" files lost !=======\") tjh_lost_textures_finder_lostScrollField;\n\n\n}\n\n\nglobal proc tjh_lost_textures_finder__selectLostedFileNodes()\n{\n\tglobal string $tjh_lost_textures_finder__LostFileNodesList[];\n\tselect -r $tjh_lost_textures_finder__LostFileNodesList;\n}\nglobal proc tjh_lost_textures_finder__DeleteLostedFileNodes()\n{\n\tglobal string $tjh_lost_textures_finder__LostFileNodesList[];\n\n\tif(size($tjh_lost_textures_finder__LostFileNodesList)>0)delete $tjh_lost_textures_finder__LostFileNodesList;\n}\n\nglobal proc tjh_lost_textures_finder__disableLostedFileLoad(string $toggle)//on | off\n{\n\tglobal string $tjh_lost_textures_finder__LostFileNodesList[];\n\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes[];\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[];\n\n\t//如果没有丢失图片则选择所有图片节点\n\tif(size($tjh_lost_textures_finder__LostFileNodesList)==0) \n\t{\n\t\ttjh_lost_textures_finder__CheckSceneFileTextures;\n\t\ttjh_lost_textures_finder__selectLostedFileNodes;\n\t\tif(size($tjh_lost_textures_finder__LostFileNodesList)==0)\n\t\t{\n\t\t\t//get all the textures \n\t\t\tstring $list_cmd = \"ls \";\n\t\t\tfor($i = 0 ;$i<size($tjh_lost_textures_finder__support_fileTypeNodes);$i++)\n\t\t\t{\n\t\t\t\tif(` tjh_lost_textures_file_type_exist_test $tjh_lost_textures_finder__support_fileTypeNodes[$i] `)$list_cmd += \" -type \" + $tjh_lost_textures_finder__support_fileTypeNodes[$i] +\" \";\n\t\t\t}\n\n\t\t\t$list_cmd += \";\";\n\t\t\t$tjh_lost_textures_finder__LostFileNodesList  = ` eval $list_cmd  `;\n\t\t}\n\t}\n\t\n\t//默认针对丢失图片节点操作\n\tstring $sel;\n\tfor($sel in $tjh_lost_textures_finder__LostFileNodesList)\n\t{\n\t\tif(` objExists ($sel+\".disableFileLoad\")`)\n                {\n                        if($toggle == \"off\")setAttr ($sel+\".disableFileLoad\") 0;\n                        if($toggle == \"on\")setAttr ($sel+\".disableFileLoad\") 1;\n                }\n\t}\n}\n\nglobal proc selectLostedFileNodes_singleSelect(string $toggle)// back | front\n{\n\tglobal string $tjh_lost_textures_finder__LostFileNodesList[];\n\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes[];\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[];\n\n\n\tif(size($tjh_lost_textures_finder__LostFileNodesList)==0)\n\t{\n\t\t\n\t\t//get all the textures \n\t\tstring $list_cmd = \"ls \";\n\t\tfor($i = 0 ;$i<size($tjh_lost_textures_finder__support_fileTypeNodes);$i++)\n\t\t{\n\t\t\tif(` tjh_lost_textures_file_type_exist_test $tjh_lost_textures_finder__support_fileTypeNodes[$i] `)$list_cmd += \" -type \" + $tjh_lost_textures_finder__support_fileTypeNodes[$i] +\" \";\n\t\t}\n\n\t\t$list_cmd += \";\";\n\t\t$tjh_lost_textures_finder__LostFileNodesList  = ` eval $list_cmd  `;\n\t\t\n\t}\n\t\n\tglobal int $tjh_lost_textures_finder__LostFileNodesList__current_selected_id ;\n\tif($tjh_lost_textures_finder__LostFileNodesList__current_selected_id <= 0 ) \n\t$tjh_lost_textures_finder__LostFileNodesList__current_selected_id  = 0;\n\n\t\n\tint $id=0;\n\tif(size ($tjh_lost_textures_finder__LostFileNodesList)>0)\t\n\t{\n\t\t$id= $tjh_lost_textures_finder__LostFileNodesList__current_selected_id % size ($tjh_lost_textures_finder__LostFileNodesList) ;\n\t\t  \n\n\t\tswitch($toggle)\n\t\t{\t \n\t\t\t\n\t\tcase \"front\" :\t\t\n\t\t\t\n\t\t\t$tjh_lost_textures_finder__LostFileNodesList__current_selected_id++;\n\t\t\tbreak;\n\t\t\t\n\t\tcase \"back\" :\n\n\t\t\t$tjh_lost_textures_finder__LostFileNodesList__current_selected_id--;\n\t\t\tbreak;\t\t\n\t\t}\n\n\t\tif($tjh_lost_textures_finder__LostFileNodesList__current_selected_id <0)\n\t\t$tjh_lost_textures_finder__LostFileNodesList__current_selected_id = size($tjh_lost_textures_finder__LostFileNodesList);\n\n\t\tselect -r  $tjh_lost_textures_finder__LostFileNodesList[$id];\n\t}\n}\n\n\n\n\n//////////////////////////////////////////////////\n/////////////// core procedures /////////////////\n//////////////////////////////////////////////////\n\n\n\n//set current path with new path,and  support the sub-directory index.\n\nglobal proc tjh_texture_finder_proc(string $newPath,int $method)\n\n// $newPath : \"c:/temp/\" \n// $method =0 : set current \n// $method =1 : copy to target \n\n{\n\ttjh_lost_textures_finder__clearResult;\n\n\t$newPath = encodeString(fromNativePath($newPath));\n\n\tglobal string $tjh_lost_textures_finder__LostResultList[];\n\tglobal string $tjh_lost_textures_finder__LostFileNodesList[];\n\t//clear $tjh_lost_textures_finder__LostResultList;\n\n\t//string $newPath=`textFieldButtonGrp -q -text tjh_lost_textures_finder_textFieldButtonGrp1`;\n\n\tif($newPath != \"c:/temp/ ( support sub-Directories searching...)\" && size($newPath)>0)\n\t{\n\n\t\t\n\n\t\tscrollField -e  -insertionPosition 0 -insertText (\"\\n\\n>>>>>>>>>>>>  Begin ! >>>>>>>>>>\\n\") tjh_lost_textures_finder_findedScrollField;\n\t\tscrollField -e -insertionPosition 0 -insertText (\"\\n\\n>>>>>>>>>>>>  Begin ! >>>>>>>>>>\\n\") tjh_lost_textures_finder_lostScrollField;\n\n\n\n\t\t//get the all files and directorier list.====================================\n\n\t\tstring $tjh_directorys[];clear $tjh_directorys;\n\t\tstring $tjh_files[];clear $tjh_files;\n\n\t\tstring $targePath=` textFieldButtonGrp -q -text tjh_lost_textures_finder_textFieldButtonGrp2 `;\n\n\n\t\tif($method ==0)\t\n\t\t{\n\t\t\t//tjh_subDir_finder $newPath $tjh_directorys $tjh_files;\n\n\t\t\tstring $pathString[];\t\t\n\t\t\ttokenize $newPath \",\"  $pathString;\n\t\t\t//print $pathString;\n\n\t\t\tstring $path;\n\t\t\tfor($path in $pathString)\n\t\t\t{\n\t\t\t\tif(size($path) > 0 && $path!=\"\")tjh_subDir_finder $path $tjh_directorys $tjh_files;\n\t\t\t}\n\t\t}\n\t\tif($method ==1)\t\n\t\t{\n\t\t\tstring $pathString[];\t\t\n\t\t\ttokenize $newPath \",\"  $pathString;\n\t\t\t//print $pathString;\n\n\t\t\tstring $path;\n\t\t\tfor($path in $pathString)\n\t\t\t{\n\t\t\t\tif(size($path) > 0 && $path!=\"\")tjh_subDir_finder $path $tjh_directorys $tjh_files;\n\t\t\t}\n\t\t\t\n\n\t\t}\n\t\t\n\t\t\n\t\t//remove the null data from $tjh_directorys and $tjh_files ==========================================\n\t\t//过滤空目录\n\t\tstring $fliterNullsTest[];\n\t\tint $fliterNullsTest_id =0;\n\t\tint $fliterNull_size = size($tjh_directorys);\n\t\tfor($i=0;$i<$fliterNull_size;$i++)\n\t\t{\n\t\t\tif(size($tjh_directorys[$i])>0)\n\t\t\t{\n\t\t\t\t$fliterNullsTest[$fliterNullsTest_id]=$tjh_directorys[$i];\n\t\t\t\t$fliterNullsTest_id++;\n\t\t\t}\n\n\t\t}\n\t\tclear $tjh_directorys;\n\t\t$tjh_directorys = $fliterNullsTest;\n\n\t\t//过滤空路径\n\t\tclear $fliterNullsTest;\n\t\t$fliterNullsTest_id =0;\n\t\t$fliterNull_size = size($tjh_files);\n\t\tfor($i=0;$i<$fliterNull_size;$i++)\n\t\t{\n\t\t\tif(size($tjh_files[$i])>0)\n\t\t\t{\n\t\t\t\t$fliterNullsTest[$fliterNullsTest_id]=$tjh_files[$i];\n\t\t\t\t$fliterNullsTest_id++;\n\t\t\t}\n\n\t\t}\n\t\tclear $tjh_files;\n\t\t$tjh_files = $fliterNullsTest;\n\n\n\t\t\n\n\t\t\n\t\t\n\t\t//test the find data.\n\t\t\n\t\t//print (\"$tjh_directorys = \"+size($tjh_directorys)+\"\\n\");\n\t\t//print $tjh_directorys;\n\t\t//print (\"$tjh_files = \"+size($tjh_files)+\"\\n\");\n\t\t//print $tjh_files;\n\t\t\n\n\t\tstring $sel;\n\t\tstring $selects[];\n\n\t\t//test the RadioButtonGrp for Search range.\n\t\tint $testSearchRange = ` radioButtonGrp -q  -select tjh_lost_textures_finder_SearchRange_RadioButtonGrp `;\n\t\t//int $LostResultListSize = size($tjh_lost_textures_finder__LostResultList);\n\n\t\t\n\t\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes[];\n\t\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[];\n\n\t\tif($testSearchRange == 1)//get all textures-list\n\t\t{\n\t\t\t//get all the textures 得到所有图片节点\n\t\t\tstring $list_cmd = \"ls \";\n\t\t\tfor($i = 0 ;$i<size($tjh_lost_textures_finder__support_fileTypeNodes);$i++)\n\t\t\t{\n\t\t\t\tif(` tjh_lost_textures_file_type_exist_test $tjh_lost_textures_finder__support_fileTypeNodes[$i] `)\n\t\t\t\t{\n\t\t\t\t\t//if($tjh_lost_textures_finder__support_fileTypeNodes[$i] != \"file\")\n\t\t\t\t\t//{\n\t\t\t\t\t\t$list_cmd += \" -type \" + $tjh_lost_textures_finder__support_fileTypeNodes[$i] +\" \";\n\t\t\t\t\t//}\n\t\t\t\t}\n\t\t\t}\n\n\t\t\t$list_cmd += \";\";\n\n\t\t\t$selects =` eval $list_cmd `;\n\n\t\t}\n\n\n\t\tif($testSearchRange == 2) //get textures-list only from the result of first searching\n\t\t{\n\t\t\t//read the memory list of lost result.\n\t\t\t$selects = $tjh_lost_textures_finder__LostFileNodesList;\n\t\t\t//**** clear memory for array ****\n\t\t\t//clear $tjh_lost_textures_finder__LostResultList;\n\t\t}\n\n\t\t\n\t\t\n\t\tint $LostResultList_id = 0;\n\n\t\t\n\n\t//main replace process ================================================\n\t\tfor($sel in $selects)\n\t\t{\n\t\t\t\n\t\t\tstring $sel_path_attr = ` tjh_lost_textures_file_type_Attr_get $sel `;\n\t\t\tstring $filePath =fromNativePath(` getAttr  $sel_path_attr `);\n\t\t\t//保存老的文件路径\n\t\t\tstring $oldfilePath =  $filePath;\n\t\t\t\n\n\t\t\tint $testFileExist=0;\n\t\t\tint $exe_toggle = 0;\n\n\t\t\t\n\t\t\t//save the old \"file\" type node's  colorSpace value into the attr \".tjh_color_Space\"\n\n\t\t\tif(` objExists ($sel + \".colorSpace\") `)  //attributeExists \"colorSpace\" $sel\n\t\t\t{\n\t\t\t\tif(!` objExists ($sel + \".tjh_colorSpace\")`)\n\t\t\t\t{\n\t\t\t\t\taddAttr -ln \"tjh_colorSpace\"  -dt \"string\"  $sel;\n\t\t\t\t\tsetAttr -e-keyable true ($sel + \".tjh_colorSpace\");\n\t\t\t\t}\n\n\t\t\t\tstring $tjh_colorSpace = ` getAttr ($sel+\".colorSpace\") `;\t\t\t\n\t\t\t\tsetAttr ($sel + \".tjh_colorSpace\")  -type \"string\"  $tjh_colorSpace;\n\t\t\t}\n\t\t\t\n\n\t\t\t\t\t\t\n\t\t\t\n\t\t\tif($testSearchRange == 1)//search all file textures \n\t\t\t{\t\t\t\t\n\t\t\t\t$exe_toggle = 1;\n\t\t\t}\n\n\t\t\t//test the file of texture exist or not.\n\t\t\tif($testSearchRange == 2)//only search the lost textures.\n\t\t\t{\t\t\t\t\n\t\t\t\t$testFileExist = ` filetest -e $filePath `;\n\t\t\t\tif($testFileExist==0)$exe_toggle = 1;\n\t\t\t\tif($testFileExist==1)$exe_toggle = 0;\n\t\t\t}\n\n\t\t\t//if file not exist and find it\n\t\t\tif( $exe_toggle == 1)\n\t\t\t{\n\n\t\t\t\tstring $oldfileString[];\n\t\t\t\tclear $oldfileString;\n\t\t\t\t//get the file name from the  path\n\t\t\t\ttokenize $oldfilePath \"/\"  $oldfileString;\n\n\t\t\t\tint $oldfileStringSize = size($oldfileString);\n\t\t\t\tif(($oldfileStringSize-1)<0) $oldfileStringSize=1;\n\t\t\t\t//string $newPath = \"sourceimages\"+\"/\"+$fileString[($fileStringSize-1)];\t\t\t\t\t\n\t\t\t\t\n\n\t\t\t\tstring $findAllfiles[];clear  $findAllfiles;\n\t\t\t\tint $findAllfiles_id = 0;\n\t\t\t\t\n\t\t\t\t//找到所有路径形似的文件（模糊逻辑：根据名称与路径的相似度判断，级别分为3级，越大越高，1:仅文件名相同即可，2：文件名和路径近似 3：路径及文件名完全相同）\n\n\t\t\t\t\n\t\t\t\tstring $result = \"\";\n                                string $resultFileName = \"\";\n                                string $udimResults[];\n                                string $udimResultNames[];\n                                int $udimResults_id = 0;\n\n\t\t\t\t//search $sel in the files of all pathes\n\t\t\t\tfor($i=0;$i<size($tjh_files);$i++)\n\t\t\t\t{\t\n\t\t\t\t\t//record the result of searching\n\t\t\t\t\t//initial temp value.\n\t\t\t\t\t//$filePath = \"\";\n\t\t\t\t\tstring $searchfileString[];\n\t\t\t\t\tclear $searchfileString;\n\t\t\t\t\t\n\n\t\t\t\t\t//get the file name from the path\n\t\t\t\t\ttokenize $tjh_files[$i] \"/\" $searchfileString;\n\t\t\t\t\tint $searchfileStringSize = size($searchfileString);\n\t\t\t\t\tif(($searchfileStringSize-1)<0) $searchfileStringSize=1;\n\n\t\t\t\t\t//match the file name of searching from the file texture list\n\t\t\t\t\t//string $testResult=`match $fileString[($fileStringSize-1)] $tjh_files[$i]`; \n\t\t\t\t\t\n\t\t\t\t\t//fliter the .mayaSwatcher file.(.swatch | .swatches)\t\n\t\t\t\t\t//通过完整路径名称判断图片路径是否吻合,\n\t\t\t\t\t//if( $filePath == $tjh_files[$i] && ` filetest -e $filePath `)\n\t\t\t\t\t\n\t\t\t\t\t//获取所有符合条件的文件路径列表,包括重名不同路径的文件\n\n\t\t\t\t\t//第一次过滤udim类贴图，v1.3.0新功能算法，判断\n                                        \n                                        string $udimCheckResults[] = ` tjh_UDIM_pics_check $oldfileString[$oldfileStringSize-1]  $searchfileString[$searchfileStringSize-1] ` ;                                      \n                                        if( $udimCheckResults[1] ==  $searchfileString[$searchfileStringSize-1] && $tjh_files[$i]!= \"\")\n                                        {\t\t\t\t\t\t\n                                                $udimResults[$udimResults_id] = $tjh_files[$i];\n                                                $udimResultNames[$udimResults_id] = $searchfileString[$searchfileStringSize-1];\n                                                $udimResults_id++;\n                                                print (\"$findFils_<udim>_result = \"+$tjh_files[$i] + \"\\n\"); \n                                                \n                                                scrollField -e  -insertionPosition 0 -insertText (\"\\n>>  <udim> \"+$tjh_files[$i]+\"   Linked OK\") tjh_lost_textures_finder_findedScrollField;\n\n                                                //获取首张符合udim的图片名，并作为图片结果\n                                                if($searchfileString[$searchfileStringSize-1]==$oldfileString[$oldfileStringSize-1] )  \n                                                {\t\n                                                        $result = $tjh_files[$i];\n                                                        $resultFileName = $oldfileString[$oldfileStringSize-1];\n                                                        //print (\"$findFils_<udim>_result = \"+$result + \"\\n\");                                               \n                                                        //break;\n                                                }\n\n                                        }\t\n\t\t\t\t\t\t\n\n\t\t\t\t\t//第二次过滤，判断非udim文件名是否匹配原图名，符合加入查询结果中。\n\t\t\t\t\tif($searchfileString[$searchfileStringSize-1]==$oldfileString[$oldfileStringSize-1] && $udimCheckResults[0] ==  \"0\")\n\t\t\t\t\t//  match \"pic01.jpg\" \"pic01.jpg\" \n\t\t\t\t\t{\t\n\t\t\t\t\t\t$result = $tjh_files[$i];\n                                                $resultFileName = $oldfileString[$oldfileStringSize-1];\n                                                print (\"$findFils_result = \"+$result + \"\\n\");                                               \n                                                //break;\n\t\t\t\t\t}\n                                        \n\n\t\t\t\t\t\n\t\t\t\t}//for end\n\n                                        \n                                //根据找到的结果文件数，执行重链接 或 拷贝 \n\n                                //test the file existing.\n                                $testFileExist = ` filetest -e $result `;\n\n                                if($method ==0)//link only\n                                {\n                                        if($testFileExist == 1)\n                                        {\n                                                string $sel_path_attr = ` tjh_lost_textures_file_type_Attr_get $sel `;\n                                                if(` objExists $sel_path_attr `) setAttr -type \"string\"  $sel_path_attr  $result;\n                                                if(` objExists ($sel+\".filterType\") `) setAttr ($sel+\".filterType\") 1;\t\n                                                //print  (\"\\n\"+$tjh_files[$i]);\t\t\t\t\t\t\t\t\t\n                                                \n                                                scrollField -e  -insertionPosition 0 -insertText (\"\\n>>  \"+$result+\"   Linked OK\") tjh_lost_textures_finder_findedScrollField;\n\n                                        }else{\n                                                scrollField -e -insertionPosition 0 -insertText (\"\\n>>  \"+$oldfilePath+\"   Not be found in  \"+$newPath+\" !\") tjh_lost_textures_finder_lostScrollField;\n                                        }\n                                }\n\n                                if($method ==1)//copy to other dir and link\n                                {\t\n                                        if($testFileExist == 1)\n                                        {\n\n                                                //拷贝udim图片\n                                                if(size($udimResults)>0)\n                                                {\n                                                  for($c=0;$c<$udimResults_id;$c++) \n                                                  {\n                                                        sysFile -copy  ( $targePath + $udimResultNames[$c] ) $udimResults[$c];\n                                                        scrollField -e  -insertionPosition 0 -insertText (\"\\n>> Copied <udim>  \"+$udimResults[$c]+\"  To  \"+( $targePath + $udimResultNames[$c]  )+\" and Linked OK\") tjh_lost_textures_finder_findedScrollField;\n\n                                                  }\n                                                }\n\n                                                //拷贝二次查找绝对同名图片\n                                                sysFile -copy  ( $targePath + $resultFileName ) $result;\t\n\n                                                \n                                                string $sel_path_attr = ` tjh_lost_textures_file_type_Attr_get $sel `;\n                                                if(` objExists $sel_path_attr `) setAttr  -type \"string\"  $sel_path_attr  ( $targePath + $resultFileName );\n                                                if(` objExists ($sel+\".filterType\") `) setAttr ($sel+\".filterType\") 1;\t\n                                                //print  (\"\\n\"+$tjh_files[$i]);\n\n                                                scrollField -e  -insertionPosition 0 -insertText (\"\\n>>  Copied \"+$tjh_files[$i]+\"  To  \"+( $targePath + $resultFileName )+\" and Linked OK\") tjh_lost_textures_finder_findedScrollField;\n                                                \n                                        }else{\n\n                                                scrollField -e -insertionPosition 0 -insertText (\"\\n>>  \"+$oldfilePath+\"   Not be found in  \"+$targePath+\" !\") tjh_lost_textures_finder_lostScrollField;\n\n                                        }\n                                }\n\n\n                                //for file node ,restore the colorSpace value.\n                                if(` objExists ($sel + \".colorSpace\") `) \n                                {\n                                        if(` objExists ($sel + \".tjh_colorSpace\")`)\n                                        {\n                                                string $tjh_colorSpace = ` getAttr ($sel+\".tjh_colorSpace\") `;\t\t\t\n                                                setAttr ($sel + \".colorSpace\")  -type \"string\"  $tjh_colorSpace;\n                                        }\n                                        \n                                }\n                                \n\n\n\n                                if($findAllfiles[0] == \"\")//process when no result\n                                {\n                                        \n                                        string $sel_path_attr = ` tjh_lost_textures_file_type_Attr_get $sel `;\n                                        $filePath =` getAttr  $sel_path_attr `;\n                                        //print (\"\\n\"+$filePath+\" not be found!\");\n                                        if($method ==0)scrollField -e -insertionPosition 0 -insertText (\"\\n>>  \"+$oldfilePath+\"   Not be found in  \"+$newPath+\" !\") tjh_lost_textures_finder_lostScrollField;\n                                        if($method ==1)scrollField -e -insertionPosition 0 -insertText (\"\\n>>  \"+$oldfilePath+\"   Not be found in  \"+$targePath+\" !\") tjh_lost_textures_finder_lostScrollField;\n                                        \n                                        //record the lost Result to global value.\n                                         $tjh_lost_textures_finder__LostResultList[$LostResultList_id] = $sel;\n                                         \n                                         $LostResultList_id ++;\n                                }\n\n\n\n\t\t\t}  //if end ----test the file of texture exist\n\t\t}//for end ----main process\n\n\n\t\ttjh_lost_textures_finder__CheckSceneFileTextures;\t\t\n\n\n\n\n\t}else{scrollField -e  -insertionPosition 0 -insertText (\"\\n\\n>>>please set the valuable path first!\\n\") tjh_lost_textures_finder_lostScrollField;}\n\n\nreturn;\n\n}\n\n\n\n\n\n\nglobal proc string  tjh_rand_Namer(int $nameLength)\n{\n\tstring $dict[]={\"a\",\"b\",\"c\",\"d\",\"e\",\"f\",\"g\",\"h\",\"i\",\"j\",\"k\",\"l\",\"m\",\"n\",\"o\",\"p\",\"q\",\"r\",\"s\",\"t\",\"u\",\"v\",\"w\",\"x\",\"y\",\"z\"};\n\tstring $finalName= \"\";\n\tfor($i=0;$i<$nameLength;$i++)\n\t{\n\t\t$finalName += $dict[int(rand(26))];\t\n\t}\n\nreturn $finalName;\n} //tjh_rand_Namer(6);\n\n\n\n//自动重命名文件程序,同时具备文件路径拆分功能,\n//检测文件是否已经存在，当存在同名文件不同路径时，对于同名文件的自动重命名\n//global proc string[] tjh_autoReNamePicFileExist() 函数返回值\n\n//$result[0]= $newName;//新自动命名文件名---带完整路径\n//$result[1]= $filePath;//源文件 路径\n//$result[2]= $fileName;//源文件 名\n//$result[3]= $fileMid;//源文件 名中间名\n//$result[4]= $fileExt;//源文件 拓展名\n\nglobal proc string[]  tjh_AutoReNameFileExists(string $checkFile)\n{\n\tstring $result[];\n\t\n\tstring $newName = $checkFile;\n\n\tstring $buffers[];\n\ttokenize $checkFile \"/\" $buffers;\n\tstring $fileNameWithExt = $buffers[size($buffers)-1];\n\tstring $filePath=$buffers[0]+\"/\";\n\t\n\tif(size($buffers)>2)\n\t{\n\t\tfor($i=1;$i<size($buffers)-1;$i++)\n\t\t{\n\t\t\t$filePath+= $buffers[$i]+\"/\";\n\t\t}\n\t}\n\n\tclear $buffers; tokenize $fileNameWithExt \".\" $buffers;\n\tstring $fileName = $buffers[0];\n\tstring $fileExt = $buffers[size($buffers)-1];\n\tstring $fileMid =\".\";\n\t\n\tif(size($buffers)>2)\n\t{\n\t\tfor($i=1;$i<size($buffers)-1;$i++)\n\t\t{\n\t\t\t$fileMid+= $buffers[$i]+\".\";\n\t\t}\n\t}\n\t\n\t$result[0]= $newName;\n\t$result[1]= $filePath;\n\t$result[2]= $fileName;\n\t$result[3]= $fileMid;\n\t$result[4]= $fileExt;\n\t\n\t\n\t\n\twhile(` filetest -e $newName `) \n\t{\n\t\t$newName = $filePath+$fileName+ \"_\"+ tjh_rand_Namer(6)+$fileMid+$fileExt;\t\t\n\t}\n\t\n\t\nreturn $result;\n\n}\n\n\n\n\n//v1.3.0 新功能：\n//检测udim图片类型，并返回所有udim图片路径列表\nglobal proc string[] tjh_UDIM_pics_check(string $targetPic,string $sourcePic)\n{\n\tstring  $result[]; \n        $result[0] = \"0\";//保存udim 类型 1-4\n        $result[1] = \"\"; //保存文件路径结果\n        \n        //udim uv图片检测，及图片拷贝\n\n\t//================= match 表达式语法参考 ============\n\t/*\n\t.\tMatches any single character\n\t*\tMatch zero or more occurances of the preceeding expression\n\t+\tMatch one or more occurances of the preceeding expression\n\t^\tMatches (anchors) the expression to the start of a line\n\t$\tMatches (anchors) the expression to the end of a line\n\t\\\tEscape character. Use this in front of a special character (e.g. '*') when you wish to match the actual character. Note that MEL strings resolve control characters that use '\\' when they are first parsed. So, to use a '\\' character in an expression, you must escape it (e.g. \"\\\\\") so that it is passed through to match.\n\t[...]\tMatches any one of the enclosed characters. All of the special pattern matching characters except \"\\\" and \"]\" lose their special meaning between the brackets and just match themselves. A pair of characters separated by \"-\" matches any character lexically between the pair, inclusive (e.g. \"[0-9]\" matches any digit). If the first character following the opening \"[\" is a \"^\" any character not enclosed is matched (e.g. \"[^ab]\" will match any character except \"a\" or \"b\"). A \"-\" can be included in the character set by putting it as the first or last character.\n\t(...)\tUsed to group part of an expression together\n\n\tmatch \"a+b+c+\" \"abbccc\";\n\t// Result: abbccc // \n\tmatch \"[A-z]+.[0-9]+.[a-z]+$\" \"Pic.1001.jpeg\" ;\n\t// Result: Pic.u0_v0.jpeg // \n\tmatch \"^[A-z]+.u[0-9]_v[0-9].[a-z]+\" \"Pic.u0_v0.jpeg\" ;\n\t// Result: Pic.u0_v0.jpeg // \n\tmatch \"u[0-9]_v[0-9]\" \"Pic.u1_v1.jpeg\" ;\n\t// Result: .1001. //\n\tmatch \".10[0-9]+.\" \"Pic.1001.jpeg\";\n\t// Result: u1_v1 //\n\tmatch \"(tjh)+\" \"tjhtjhtjh.1001.jpeg\";\n\t// Result: tjhtjhtjh // \n\tmatch \"[tjh&]*\" \"tjhtjhtjh.1001.jpeg\";\n\t// Result: tjhtjhtjh //\n\tmatch \"[^t][a-z]+\" \"tjhtjhtjh.1001.jpeg\";\n\t// Result: jhtjhtjh //\n\t*/\n\t//==============================================\n\n        string $targetFileNames[];\n        tokenize $targetPic \"._\" $targetFileNames;\n\n        string $sourceFileNames[];\n        tokenize $sourcePic \"._\" $sourceFileNames;\n\n        int $sourceSizeName = size($sourceFileNames);\n        if($sourceSizeName <= 0) $sourceSizeName =1;\n\n        //排除  .swatch 文件\n        if($sourceSizeName>0 && $sourceFileNames[$sourceSizeName-1]!= \"swatch\")\n        {\n\n                // 优化算法II\n                //检测 源图片名中是否含有  .1XXX.  uX_vX  <udim> <UVTILE> 字符\n                // Pic.tt_y1.1001.jpeg   Pic.1001.tif\n                if( ` match ($targetFileNames[0]+\"[_.a-zA-Z0-9]*1[0-9][0-9][0-9][_.a-zA-Z]+\")  $targetPic ` == $targetPic) $result[0] = \"1\";\n                // Pic.tt_y1.u1_v2.jpeg   Pic_u1_v2.exr\n                if( ` match ($targetFileNames[0]+\"[_.a-zA-Z0-9]*u[0-9]_v[0-9][_.a-zA-Z]+\")  $targetPic ` == $targetPic)  $result[0] = \"2\";\n                // Pic.tt_y1.<udim>.jpeg   Pic_<udim>.exr\n                if( ` match ($targetFileNames[0]+\"[_.a-zA-Z0-9]*<udim>[_.a-zA-Z]+\")  $targetPic ` == $targetPic)  $result[0] = \"3\";\n                // Pic.tt_y1.<UVTILE>.jpeg   Pic_<UVTILE>.exr\n                if( ` match ($targetFileNames[0]+\"[_.a-zA-Z0-9]*<UVTILE>[_.a-zA-Z]+\")  $targetPic ` == $targetPic)  $result[0] = \"4\";\n\n\n                //根据udim类型检测源图片是否符合条件\n                if(  $result[0] == \"1\" || $result[0] == \"3\") \n                {\n                       if(  ` match ($targetFileNames[0]+\"[_.a-zA-Z0-9]*1[0-9][0-9][0-9][_.a-zA-Z]+\")  $sourcePic ` == $sourcePic) \n                        $result[1] = $sourcePic;\n                }\n\n                 if(  $result[0] == \"2\" || $result[0] == \"4\") \n                {\n                       if(  ` match ($targetFileNames[0]+\"[_.a-zA-Z0-9]*u[0-9]_v[0-9][_.a-zA-Z]+\")  $sourcePic ` == $sourcePic) \n                        $result[1] = $sourcePic;\n                }\n        }\n\n\nreturn $result;\n}\n\n\n//v1.3.0 新功能：\n//检测udim图片所在路径下，所有该路径下找到的udim图片完整路径列表\nglobal proc string[] tjh_UDIM_get_allUdimPics_From_picPath(string $targetPicWithPath)\n{\n\tstring  $result[]; \n        int $result_id = 0; \n\n        //获取图片目录下所有文件名列表\n        string $targetPicName =  basename( $targetPicWithPath, \"\" );\n        string $targetPicDir = dirname($targetPicWithPath);\n        $targetPicDir = substituteAllString($targetPicDir,\"//\",\"/\");\n\n        string $tjh_directorys[];clear $tjh_directorys;\n\tstring $tjh_files[];clear $tjh_files;\n        if(size($targetPicDir) > 0 && $targetPicDir!=\"\")tjh_subDir_finder $targetPicDir $tjh_directorys $tjh_files;\n\n        //加测目录中的 udim 文件，并提取符合规范的图片呢文件，到输出结果变量\n        if(size($tjh_files)>0)\n        {                \n                for($i=0;$i<size($tjh_files);$i++) \n                {\n                       string $sourcePicName =  basename( $tjh_files[$i], \"\" );\n                       string $testUdimResults[]= ` tjh_UDIM_pics_check $targetPicName $sourcePicName `;\n                       if($testUdimResults[1] == $sourcePicName && $tjh_files[$i]!= \"\" ) \n                       {\n                        $result[$result_id] =  $tjh_files[$i];\n                        $result_id++;\n                       }\n                }      \n               \n        }\n\n\nreturn $result;\n}\n\n\n\n//核心递归算法：主运算搜索文件程序\n//实现在函数形参中实现全局变量的\"输出\"功能，可以使用数组列表的方式。否则形参都只能作为函数自设\"输入类\"局部变量使用。\n\nglobal proc tjh_subDir_finder(string $path,string $outputDirectorys[],string $outputFilesList[])\n{\t\n\tstring $fileList[];\t\n\t$fileList=` getFileList -folder $path`;\n\tstring $file;\n\t\n\t//string $directorys[];\n\tint $sizeDirectorys = size($outputDirectorys);\n\tint $sizeFilesList = size($outputFilesList);\n\n\t//judge whether file is or not and record\n\tfor($file in $fileList)\n\t{\n\t\t\n\t\t//record paths to $outputFilesList[] \n\t\tif(` filetest -f ($path+\"/\"+$file) `)//judge whether file is or not\n\t\t{\n\t\t\t$sizeFilesList++;\n\t\t\t$outputFilesList[$sizeFilesList]=$path+\"/\"+$file;\n\t\t\t\t\n\t\t\t\n\t\t}\n\t}\n\n\t//judge whether directory is or not and record\t\n\tfor($file in $fileList)\n\t{\t\n\t\t//record paths to $outputDirectorys[] \n\t\tif(` filetest -d ($path+\"/\"+$file) `)//judge whether directory is or not\n\t\t{\n\t\t\t\n\t\t\t$sizeDirectorys++;\n\t\t\t$outputDirectorys[$sizeDirectorys]=$path+\"/\"+$file+\"/\";\n\t\t\t\n\t\t\t\n\n\t\t\t//####递归算法 recursion process ####\n\t\t\ttjh_subDir_finder $outputDirectorys[$sizeDirectorys] $outputDirectorys $outputFilesList;\t\t\t\n\t\t\t\n\t\t}\n\n\t\t\n\t}\n\n\t//print $outputDirectorys;\n\t//print \"\\n\\n\";\n\t//print $outputFilesList;\n\t//print \"one loaded !\\n\\n\";\n\n}\n\n\n\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n//////////////////////////////////////////////////////                        ///////////////////////////////////////////////////////////\n////////////////////////////////////////////////////// tjh_scene_package      ///////////////////////////////////////////////////////////\n//////////////////////////////////////////////////////                        ///////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n\n//coded by tiancg on sep 2014-2017.\n\n//tjh_scene_package v1.1\n//this procedure is apply presets for Multi-nodes quickly.\n//此工具可以快速的创建节点preset，并快速赋予多个选择节点。填补了maya中的功能空白\n//uage: first save a preset,and then select the other nodes for apply.\n\n\n\nglobal string $tjh_scene_package__version = \"v 1.1  2014-2017\";\n\n\n\n//UI.\nglobal proc tjh_scene_package()\n{\n\n\n// get source path of this mel.\n\t/*\n\tstring $srcPath = `whatIs \"tjh_scene_package\"`;\t\n\tint $size = size($srcPath) ;\n\t$srcPath = `substring $srcPath 25 $size`;\n\t//$srcPath = dirname( $srcPath );\n\t//$srcPath = toNativePath( $srcPath );\n\tprint $srcPath;\n\n\tglobal string $tjh_scene_package__global_path=\"\";\n\t$tjh_scene_package__global_path = $srcPath;\n\t*/\n\t\n\t//create the xpm image of title\n\t//tjh_scene_package_title;\n\t//global string $imagePath_rubbishs_fall_title;\n\n\tif (`windowPref -exists tjh_scene_package_tool_window`)\n\t\t    windowPref -remove  tjh_scene_package_tool_window;     \n\n\t\t\t  \n\t\t\t  if (`window -exists tjh_scene_package_tool_window`)\n\t\t\t  deleteUI tjh_scene_package_tool_window;\n\n\n\twindow -menuBar true -w 340 -h 140 tjh_scene_package_tool_window;\n\n\t\t\t//menu -label \"Language\" tjh_scene_package__menu_Language;\n\t\t\t//menuItem -label \"chinese\" -c \"tjh_scene_package__language chinese\" tjh_scene_package__menuItem_chinese;\n\t\t\t//menuItem -label \"english\" -c \"tjh_scene_package__language english\" tjh_scene_package__menuItem_english;\n\n\n\t\t\t//menu -label \"Help\" tjh_scene_package__menu_Help;\n\t\t\t//menuItem -label \"Help...\" -c \"tjh_scene_package_help\" tjh_scene_package__menuItem_Help;\n\n\t\t\tmenu -label \"Help\" ;\n\t\t\tmenuItem -label \"Help...\" -c \"tjh_scene_package_help\";\n\n\t\t\tmenu -label \"Blog\" tjh_scene_package__menu_Blog;\n\t\t\tmenuItem  -label \"Goto Blog...\" -c \"showHelp -absolute \\\"https://blog.csdn.net/tian0000hai\\\"\" tjh_scene_package__menuItem_Blog;\n\t\t\tmenu -label \"Tools\" ;\n\t\t\tmenuItem  -label \"Restore texture and files to old paths\" -c \"tjh_scene_package__projectTexturesPackage_restore\" ;\n\n\tcolumnLayout;\t\t\t\t\t\t\n\t\t\t//separator -h 10 -w 340;\n\n\t\t\t//image -w 340 -h 66  -image $imagePath_rubbishs_fall_title;\n\n\t\t\t//separator -h 5 -w 340;\n\t\t\t\n\t\t\tglobal string $tjh_scene_package__version;\n\t\t\t\n\t\t\tseparator -h 10 -w 340;\n\n\t\t\ttext   -fn boldLabelFont  -al \"center\" -l (\"  tjh  Scene  package  \"+$tjh_scene_package__version) tjh_scene_package__text_logo ;\n\t\t\t\n\t\t\tseparator -h 10 -w 340;\n\n\n\t\t\t//text     -al \"left\" -l \"  This procedure can apply preset for \\n  batch.\\nUsage:\\n 2.select the nodes will be apply preset.\\n and set the apply button. \"  tjh_scene_package__text_intro;\n\t\t\t//text     -al \"right\" -l \"  effect of rubbishs falling down. \";\n\n\t\t\tseparator -h 5 -w 340;\n\n\t\t\ttext     -al \"right\" -l \"  Programmer:tiancg  \" tjh_scene_package__text_programmer;\n\t\t\ttext     -al \"right\" -l \"  2014-9-16   \" tjh_scene_package__text_date;\n\n\t\t\tseparator -h 10 -w 340;\n\n\t\t\ttext -al \"left\" -l \"  1.Fill the new package Dir Name in textfield. and set the project dir. \" ;\n\t\t\t\n\t\t\tseparator -h 10 -w 340;\n\n\t\t\trowLayout -numberOfColumns 3\n\t\t\t    -columnWidth3 100 130 100\n\t\t\t    -adjustableColumn 2\n\t\t\t    -columnAlign  1 \"left\"\n\t\t\t    -columnAttach 1 \"both\"     0\n\t\t\t    -columnAttach 2 \"both\"  0\n\t\t\t    //-columnAttach 3 \"both\"  0\n\t\t\t    ;\n\t\t\t    text -al \"right\" -l \"New package Dir :\" ;\n\n\t\t\t    string $file = ` file -q -sceneName `;\n\t\t\t    string $filePath = dirname($file);\n\t\t\t    textField \n\t\t\t\t    -editable 1\t\t\t\t    \n\t\t\t\t    -changeCommand \"tjh_scene_package_fromNativePath_toMayaPath_for_TextField tjh_scene_package_textField_NewPackageDir\"\n\t\t\t\t    //-text \"New_Package_Dir\"\n\t\t\t\t    -text $filePath\n\t\t\t\t    tjh_scene_package_textField_NewPackageDir;\n\n\t\t\t    \n\t\t\t    button  -w 100 -backgroundColor 0.4 0.7 0.4 -label \"Set...\"  -command  \"tjh_scene_package__setNewPackagePath;\" tjh_scene_package_button_setProjectDir;\n\t\t\t    setParent ..;\n\n\t\t\t\n\t\t\tseparator -h 10 -w 340;\n\t\t\t\n\t\t\ttext -wordWrap 1 -al \"left\" -l \"  2.Collect the all files and textures to the new directory And Auto link the pic paths. \" ;\n\n\t\t\tseparator -h 10 -w 340;\n\n\t\t\tbutton  -w 340 -backgroundColor 0.5 0.5 1.0 -label \"(Offline Download:)  Package  All  Files  And Link pic\"  -command  \"tjh_scene_package__projectTexturesPackage (`textField -q -text  tjh_scene_package_textField_NewPackageDir`)\" tjh_scene_package_button_collectFiles;\n\n\t\t\t\n\t\t\tseparator -h 10 -w 340;\n\n\t\t\ttext -al \"left\" -l \"  3.Open the Package directory. \" ;\n\n\t\t\tseparator -h 10 -w 340;\n\t\t\t\n\t\t\t\n\t\t\tbutton  -w 340 -backgroundColor 0.7 0.7 0.7 -label \"Open  Package  Directory\"  -command  \"string $newDir = ` textField -q -text  tjh_scene_package_textField_NewPackageDir `;tjh_scene_package__openNewPackageDir($newDir); \" tjh_scene_package_button_openDir;\n\n\t\t\tseparator -h 10 -w 340;\n\n\n\n\tshowWindow;\n\n\t//set initial language for UI.\n\n\t//tjh_scene_package__language \"english\";\n\n\n}\n\nglobal proc tjh_scene_package_fromNativePath_toMayaPath_for_TextField(string $textField)\n{\t\n\t/*\n\t//method I: python\n\tpython(\"import maya.cmds as cm\"  );\n\tpython(\"field = cm.textField('\"+$textField+\"', query =1, text = 1)\");\n\tpython(\"field = field.replace('\\\\\\\\','\\/')\");\n\tpython(\"cm.textField('\"+$textField+\"', edit =1, text = field)\");\n\t*/\n\n\t//method II: mel\n\tstring $path = ` textField -q -text $textField `;\n\t$path = encodeString(fromNativePath($path));\n\ttextField -e -text $path $textField ;\n\t\n}\n\n\nglobal proc tjh_scene_package_fromNativePath_toMayaPath_for_textFieldButtonGrp(string $textFieldButtonGrp)\n{\t\n\t/*\n\t//method I: python\n\tpython(\"import maya.cmds as cm\"  );\n\tpython(\"field = cm.textFieldButtonGrp('\"+$textFieldButtonGrp+\"', query =1, text = 1)\");\n\tpython(\"field = field.replace('\\\\\\\\','\\/')\");\n\tpython(\"cm.textFieldButtonGrp('\"+$textFieldButtonGrp+\"', edit =1, text = field)\");\n\t*/\n\n\t//method II: mel\n\tstring $path = ` textFieldButtonGrp -q -text $textFieldButtonGrp `;\n\t$path = encodeString(fromNativePath($path));\n\ttextFieldButtonGrp -e -text $path $textFieldButtonGrp ;\n\t\n}\n\n\n\n\nglobal proc tjh_scene_package__setNewPackagePath()\n{\n\t string $result_m[];\n\tstring $result;\n\tstring $basicFilter = \"*.*\";\n\tint $oldVersionTest = `tjh_maya_Old_vision_test`;\n\n\tif(!$oldVersionTest)\n\t{\n\t\t$result_m = `fileDialog2 -fileMode 3 -fileFilter $basicFilter -dialogStyle 2`;\n\t\t$result = $result_m[0]; \n\t}\n\n\tif($oldVersionTest)\n\t{\n\t\t$result = dirname(`fileDialog -dm $basicFilter`);\n\t}\n\n\tif($result != \"\")textField -e -text $result tjh_scene_package_textField_NewPackageDir;\t \n\t \n}\n\n\n//===== core process ===========\nglobal proc tjh_scene_package__projectTexturesPackage(string $newPath)\n\n{\n\t//save the old textures path.\n\ttjh_lost_textures_finder__saveOldPath;\n\n\t//string $projectFilePath=` workspace -q -rd `;\n\t$newPath = encodeString(fromNativePath($newPath));\n\t\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes[];\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[];\n\n\t//get the textures by all tex-node types except maya-\"file\" node.\n\tstring $otherTypeFileNodes[];\n\tstring $list_cmd = \"ls \";\n\tfor($i = 0 ;$i<size($tjh_lost_textures_finder__support_fileTypeNodes