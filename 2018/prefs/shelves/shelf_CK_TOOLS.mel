        -command "/*\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n//////////////////////////////////////////////////////                          /////////////////////////////////////////////////////////\n////////////////////////////////////////////////////// tjh_lost_textures_finder /////////////////////////////////////////////////////////\n//////////////////////////////////////////////////////                          /////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n\n////////////////////////////////////////////////////////////////////\n\n//Coded by tiancg in 2013-2020.\n\n///////////////////////////////////////////////////////////////////////\n\n//this procedure can find the textures that be losted with automaticly.//\n\n/////////////////////////////////////////////////////////////////////////\n\n本插件主要用于对maya动画大型场景贴图出现大量错误时，用于快速高效智能修复\n错误链接之用途。\n\n///////////////////////////////////////////////////////////////////////\n\n在Maya中，我们会遇到搜索文件夹的任务，遗憾的是，Maya中为我们提供的\ngetFileList 语句只能获取一层的文件夹目录，那么，如何访问主文件夹中\n所以的子目录呢？\n\n如图：001.jpg\n\n其实，在编程逻辑中，早就为我们提供了类似问题的解决方案。\n这个技术叫做“递归”算法。即：一个程序重复调用自身的过程。\n\n最近我写了一个在maya中自动搜寻丢失贴图的程序，用到了如下的\n递归算法。先解释一下下面mel程序的使用方法：\n\ntjh_subDir_finder <搜索的主文件夹> <输出所有目录> <输出所有文件>;\n\n使用方法如下：\n\n//定义声明用于保存所有子文件夹的数组\nstring $folders[];\n\n//定义声明用于保存所有子文件夹的数组\nstring $files[];\n\n//将对\"E:/dir0/\"目录下搜索到的所有子文件夹保存在\n//$folders中，所有文件保存到$files中。\ntjh_subDir_finder \"E:/dir0/\" $folders $files;\n\n//输出结果到屏幕\nprint $folders;\nprint files;\n\n\n使用如此的简单，哈哈。\n下面看程序源码如下：\n分析见注释：\n*/\n\n\nglobal string $versionIntoAndUpdate = \"\\n\";\n\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"////// 更新说明：///////////////////////////////////////\\n\";\n$versionIntoAndUpdate += \" \\n\";\n$versionIntoAndUpdate += \"v1.3.0  2020-06-21  \t\\n\";\n$versionIntoAndUpdate += \"1)新增udim 图片查找 tjh_UDIM_pics_check 程序功能 .\\n\";\n$versionIntoAndUpdate += \"2)新增场景文件 udim图片打包功能 .\t\\n\";\n$versionIntoAndUpdate += \"3)支持Redshift 、Arnold 、Vray 、Software四种渲染器的图片查找检测与重链 打包功能.\t\\n\\n\";\n$versionIntoAndUpdate += \"v1.2.0  2019-02-27  \t\\n\";\n$versionIntoAndUpdate += \"1)新增file节点色彩空间存储与恢复功能。 \\n\";\n$versionIntoAndUpdate += \"2)新增自动检索并自动设置当前项目sourceimages目录功能。\\n\";\n$versionIntoAndUpdate += \"3)列表UI高度缩放功能，方便窗口大小控制。\\n\";\n$versionIntoAndUpdate += \"4)增加了更多系统图片节点类型支持。\\n\\n\";\n$versionIntoAndUpdate += \"v1.1.0  2017-8-6  增加了 工程文件 及贴图打包工具 的 源文件 恢复功能，便于在贴图丢失后，\\n\";\n$versionIntoAndUpdate += \"重新恢复路径，注意：此操作不建议在原有文件服务器操作。会损坏服务器原有磁盘其他文件，建议在自己硬盘上建立文件服务器映射，本地模拟服务器路径，不对服务器造成\\n\";\n$versionIntoAndUpdate += \"物理覆盖，建议服务器在专业硬盘修复公司做全面的文件恢复。\\n\\n\";\n$versionIntoAndUpdate += \"v1.0.9  2017-05-20  增加了mental ray,arnold,redshift 特有的文件节点类型的搜索功能，提供了拓展节点类搜索预设功能。支持的文件节点类型有：file, psdFileTex , imagePlane , RedshiftEnvironment , RedshiftNormalMap , RedshiftDomeLight , RedshiftSprite , RedshiftCameraMap , mentalrayTexture 。\\n\\n\";\n$versionIntoAndUpdate += \"v1.0.8  2017-04-15 增加了贴图原始路径保存功能，便于恢复之前修改过的路径。\\n\\n\";\n$versionIntoAndUpdate += \"v1.0.7  2016-08-08  修复了核心搜索路径算法错误，避免没不同格式“\\“,“/“下出现不能替换图片问题。增加了工程打包工具。修复了拷贝文件时，因新命名图片，造成文件重复的算法。即不对已存在图片再次进行拷贝。只对目标文件夹中没有的文件，进行拷贝。\\n\\n\";\n$versionIntoAndUpdate += \"v1.0.6  2015-10-17  修复了核心递归程序的丢失搜索结果的错误，添加了默认搜索中的，添加搜索路径按钮功能。\\n\\n\";\n$versionIntoAndUpdate += \"v1.0.5  2014-10-01 修复了切换路径指定操作operation选项时，UI按钮文字消失的bug。添加了全局语言检测变量。\\n\\n\";\n$versionIntoAndUpdate += \"v1.0.4  2013-08-07 \\n\";\n$versionIntoAndUpdate += \"1）更新的核心程序 tjh_subDir_finder 的文件 及 文件夹识别 命令 ，使用 “filetest“ mel 命令 替代了原来单纯判断文件名构成“.“的方法，更加的精确。\\n\";\n$versionIntoAndUpdate += \"2）更新了界面主菜单的布局，增加了版本说明菜单。\\n\";\n$versionIntoAndUpdate += \"3）更新了查找算法，增加了现有连接判断，对于现有已经连接成功的图片自动跳过，不再重新查找，提升整体执行效率。\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"///// 使用说明： ///////////////////////////////////////////\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"特点：\\n\";\n$versionIntoAndUpdate += \"支持将子路径添加到搜索自动链接丢失的纹理文件。更多免费的maya插件https://blog.csdn.net/tian0000hai\\n\";\n$versionIntoAndUpdate += \"操作系统：32位/64位窗口\\n\";\n$versionIntoAndUpdate += \"玛雅版本：2011-2017\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"1、安装：\\n\";\n$versionIntoAndUpdate += \"将文件dir解压到喜欢。拖拽mel进入maya视图并将跳转UI窗口将解压缩文件复制到您的maya脚本董事和在maya命令脚本行中，输入“tjh_lost_textures_finder_Main”代码并设置enter键。\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"2.用途：\\n\";\n$versionIntoAndUpdate += \"一切使用方法都很简单，没有发言权更多第一个添加查找路径。点击搜索！\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"//////Update Description:///////////////////////////////////////\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"v1.3.0 2020-06-21\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"1) New < udim > image search TJH_ UDIM_ pics_ Check program function\\n\";\n$versionIntoAndUpdate += \"2) New scene file < udim > picture packing function\\n\";\n$versionIntoAndUpdate += \"3) Support redshift, Arnold, Vray, software four kinds of renderers picture search detection and heavy chain packaging function\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"v1.2.0 2019-02-27\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"1) New file node color space storage and recovery function.\\n\";\n$versionIntoAndUpdate += \"2) New automatic retrieval and automatic setting of the current project sourceimages directory function.\\n\";\n$versionIntoAndUpdate += \"3) High zoom function of list UI, convenient for window size control.\\n\";\n$versionIntoAndUpdate += \"4) More system picture node type support has been added.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"V1.1.0 2017-8-6 added the source file recovery function of the engineering file and map packaging tool to facilitate the recovery of the map after it is lost,Restore the path again, note: this operation is not recommended on the original file server. Other files on the original disk of the server will be damaged. It is recommended to establish a file server mapping on your own hard disk, simulate the server path locally, and do not cause damage to the server\\n\";\n$versionIntoAndUpdate += \"For physical coverage, it is recommended that the server do a comprehensive file recovery in a professional hard disk repair company.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"V1.0.9 on May 20, 2017, the search function of file node types unique to mental ray, Arnold and redshift was added, and the function of expanding node class search preset was provided. The supported file node types are: file, psdfiletex, imageplane, redshiftenvironment, redshiftnormalmap, redshiftdomelight, redshiftsprite, redshiftcameramap, mentalraytexture.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"V1.0.8 2017-04-15 added the map original path saving function to facilitate the recovery of previously modified paths.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"V1.0.7 2016-08-08 fixed the core search path algorithm error to avoid the problem of unable to replace pictures under '\\' and '/' without different formats. Added engineering packaging tools. Fixed the algorithm of duplicate files caused by new named pictures when copying files. That is, the existing image will not be copied again. Copy only the files that are not in the target folder.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"V1.0.6 2015-10-17 fixed the error of missing search results in the core recursive program, added the function of “add search path button“ in the default search.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"V1.0.5 2014-10-01 fixes the bug that the UI button text disappears when the switch path specifies the operation option. Added global language detection variable.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"v1.0.4 2013-08-07\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"1) Updated core program TJH_ subDir_ The file and folder identification command of finder uses the “filetest“ Mel command to replace the original method of simply judging the file name to form “.“, which is more accurate.\\n\";\n$versionIntoAndUpdate += \"2) The layout of the main menu of the interface is updated and the version description menu is added.\\n\";\n$versionIntoAndUpdate += \"3) Update the search algorithm, add the existing connection judgment, automatically skip the existing pictures that have been successfully connected, and do not search again, so as to improve the overall execution efficiency.\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"/////Instructions:///////////////////////////////////////////\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"characteristic:\\n\";\n$versionIntoAndUpdate += \"Support for adding sub paths to search for automatically linked missing texture files. More free Maya plug-ins https://blog.csdn.net/tian0000hai\\n\";\n$versionIntoAndUpdate += \"Operating system: 32-bit / 64 bit window\\n\";\n$versionIntoAndUpdate += \"Maya version: 2011-2017\\n\";\n$versionIntoAndUpdate += \"\\n\";\n$versionIntoAndUpdate += \"One. Installation:\\n\";\n$versionIntoAndUpdate += \"Unzip the file dir to your liking. Drag Mel to the Maya view and jump to the UI window to copy the extracted file to your Maya script director and enter “TJH“ in the Maya command script line_ lost_ textures_ finder_ Main “code and set the Enter key.\\n\";\n$versionIntoAndUpdate += \"2. Purpose:\\n\";\n$versionIntoAndUpdate += \"All of them are simple to use, and there is no more right to say the first adding path. More important than search! Good! so comfortable! You can test it now!\\n\";\n\n\n\n\n\n//////////////////////////// 初始化信息 ///////////////////////////////////\n\n//tjh_lost_textures_finder;\nglobal int $tjh_lost_textures_language;$tjh_lost_textures_language = 1;//1:chinese | 2:english\n\n//Version updata.\nglobal string $tjh_lost_textures_finder_plugin_version = \"1.3.0\";\nglobal string $tjh_lost_textures_finder_plugin_updataTime = \"2020-06-21\";\nglobal string $tjh_lost_textures_finder_web_blog_address = \"https://blog.csdn.net/tian0000hai\";\n\n//file types define registered. 文件类型节点注册初始化\n\n\t//maya system file list registered 系统注册的文件节点列表\n\t\n\t//print ` filePathEditor -query -listRegisteredTypes `;\n\n\n\n\n\tglobal string $tjh_lost_textures_finder__register_fileType[];\n\t$tjh_lost_textures_finder__register_fileType={\t\t\n                /* ------- Redshift textures --------- */\n                \"RedshiftBokeh.dofBokehImage\",\n\t\t\"RedshiftCameraMap.tex0\",\n\t\t\"RedshiftDomeLight.tex0\",\n\t\t//\"RedshiftDomeLight.tex1\",\n\t\t\"RedshiftEnvironment.tex0\",\n\t\t//\"RedshiftEnvironment.tex1\",\n\t\t//\"RedshiftEnvironment.tex2\",\n\t\t//\"RedshiftEnvironment.tex3\",\n\t\t//\"RedshiftEnvironment.tex4\",\n\t\t\"RedshiftIESLight.profile\",\n\t\t\"RedshiftLensDistortion.LDimage\",\n\t\t\"RedshiftLightGobo.tex0\",\n\t\t\"RedshiftNormalMap.tex0\",                \n\t\t//\"RedshiftProxyMesh.fileName\",\n\t\t\"RedshiftSprite.tex0\",\n\t\t//\"RedshiftVolumeShape.fileName\",                \n                /*   ------- VRay textures -------    */\n                \"VRayLightIESShape.iesFile\",\n\t\t//\"VRayMesh.fileName2\",\n\t\t//\"VRayMtlGLSL.fileName\",\n\t\t//\"VRayMtlMDL.filename\",\n\t\t//\"VRayMtlOSL.fileName\",\n\t\t\"VRayPtex.ptexFile\",\n\t\t//\"VRayScannedMtl.file\",\n\t\t//\"VRayScene.FilePath\",\n\t\t//\"VRayScene.parameterOverrideFilePath\",\n\t\t//\"VRaySettingsNode.causticsAutoSaveFile2\",\n\t\t//\"VRaySettingsNode.causticsFile2\",\n\t\t//\"VRaySettingsNode.imap_autoSaveFile2\",\n\t\t//\"VRaySettingsNode.imap_fileName2\",\n\t\t//\"VRaySettingsNode.lc_autoSaveFile\",\n\t\t//\"VRaySettingsNode.lc_fileName\",\n\t\t//\"VRaySettingsNode.pmap_autoSaveFile2\",\n\t\t//\"VRaySettingsNode.pmap_file2\",\n\t\t//\"VRaySimbiont.file\",\n\t\t//\"VRayTexGLSL.fileName\",\n\t\t//\"VRayTexOCIO.ocioConfigFile\",\n\t\t//\"VRayTexOSL.fileName\",\n\t\t//\"VRayVRmatMtl.fileName\",\n\t\t//\"VRayVolumeGrid.inFile\",\n\t\t /* -------  Arnold textures ------- */\n                \"aiImage.filename\",\n\t\t\"aiPhotometricLight.aiFilename\",\n\t\t//\"aiStandIn.dso\",\n\t\t//\"aiVolume.filename\",\n\t\t /* -------  Maya textures -------  */\n                //\"assemblyDefinition\",\n\t\t//\"assemblyReference\",\n\t\t//\"audio.filename\",\n\t\t\"file.fileTextureName\",                \n\t\t\"psdFileTex.fileTextureName\",\n\t\t\"imagePlane.imageName\",\n\t\t\"mentalrayTexture.fileTextureName\"//,\t\t\n\t\t//\"reference\",\n\t\t//\"xgmDescription\",\n\t\t//\"xgmPalette\"\n\t\t};\n\t\n\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes[]; \n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[];\n\tstring $singleNode;\n\tint $id = 0;\n\tfor($singleNode in $tjh_lost_textures_finder__register_fileType)\n\t{\n\t\tstring $split[];\n\t\ttokenize $singleNode \".\" $split;\n\t\tif(size($split)==2)\n\t\t{\n\t\t\t$tjh_lost_textures_finder__support_fileTypeNodes[$id]=$split[0];\n\t\t\t$tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[$id]=$split[1];\n\t\t}\n\t\t\n\t\t/*\n\t\tif(size($split)==1)\n\t\t{\n\t\t\t$tjh_lost_textures_finder__support_fileTypeNodes[$id]=$split[0];\n\t\t\t$tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[$id]=\"\";\t\t\n\t\t}\n\t\t*/\n\t\t$id++;\n\t}\n\n\n////////////////////////////////////////////////////////////////////////////////\n\n\n\n//test the file node exists for all kinds texture-file nodes in renderer like arnold redshift mentalray etc.\nglobal proc int tjh_lost_textures_file_type_exist_test(string $nodeName)\n{\n\t\n\tint $exist = 0;\n\n\t//get all node types. 当没哟通过 filePathEditor 命令注册的节点属性时，推荐使用下面方法，此法对于拥有第三方插件新节点的情况有利。\n\tstring $allNodes[] =` allNodeTypes `;\n\n\t//get the sys file-type nodes.当使用filePathEditor 命令注册过节点属性时，推荐使用下面方法，此法的运行效率较高\n\t//string $allNodes[] = ` filePathEditor -query -listRegisteredTypes `;\n\n\tstring $node;\n\tfor($node in $allNodes)\n\t{\n\t\tif(` match $nodeName $node ` == $node)\n\t\t{\n\t\t\t$exist = 1;\n\t\t\tbreak;\n\t\t}\n\t}\n\n\treturn $exist;\n}\n\n//get attribute of file node path .\nglobal proc string tjh_lost_textures_file_type_Attr_get(string $nodeName)\n{\n\t\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes[];\n\tglobal string $tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[]; \n\n\tstring $nodeFilePathAttr = \"\";\n\n\tstring $nodeType = ` nodeType $nodeName `;\n\t\n\tfor($i=0 ;$i <size($tjh_lost_textures_finder__support_fileTypeNodes);$i++)\n\t{\n\t\tif(` match $nodeType $tjh_lost_textures_finder__support_fileTypeNodes[$i] ` == $tjh_lost_textures_finder__support_fileTypeNodes[$i])\n\t\t{\n\t\t\t$nodeFilePathAttr = $nodeName + \".\"+$tjh_lost_textures_finder__support_fileTypeNodes_fileAttrs[$i];\n\t\t\tbreak;\n\t\t}\n\t}\n\n\treturn $nodeFilePathAttr;\n}\n\n//////// 语言翻译 //////////////////\n\nglobal proc tjh_lost_textures_language_english()\n{\n\n\tglobal int $tjh_lost_textures_language;$tjh_lost_textures_language=2;\n\t//menu\n\tmenu -e -l \"Language\" tjh_lost_textures_menu___language;\n\tmenuItem -e -l \"chinese\" tjh_lost_textures_menuItem___chinese;\n\tmenuItem -e -l \"english\" tjh_lost_textures_menuItem___english;\n\tmenu -e -l \"Blog\" tjh_lost_textures_menu___blog;\n\tmenuItem -e -l \"Goto Blog...\" tjh_lost_textures_menuIte