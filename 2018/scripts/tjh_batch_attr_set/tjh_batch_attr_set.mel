global proc  tjh_batch_attr_set___Add_Attr_into_textField(string $attrType,string $attr_name_textField)
{
	 if($attrType == "Vector")
	 {
		textField -e -text  ( "Add_"+$attrType +" 0 0 0 ") ($attr_name_textField+"Value");
	 }

	  if($attrType == "Int")
	 {
		textField -e -text  ( "Add_"+$attrType +" -10 0 10 " ) ($attr_name_textField+"Value");
	 }

	  if($attrType == "Float")
	 {
		textField -e -text  ( "Add_"+$attrType +" -1.0 0 1.0 " ) ($attr_name_textField+"Value");
	 }

	  if($attrType == "String")
	 {
		textField -e -text  ( "Add_"+$attrType +" null " ) ($attr_name_textField+"Value");
	 }

	  if($attrType == "Boolean")
	 {
		textField -e -text  ( "Add_"+$attrType +" true " )($attr_name_textField+"Value");
	 }

	  if($attrType == "Enum")
	 {
		textField -e -text  ( "Add_"+$attrType +" one two three " ) ($attr_name_textField+"Value");
	 }

	  if($attrType == "Delete")
	 {
		textField -e -text  ( "Delete_Attr" ) ($attr_name_textField+"Value");
	 }
}



global proc  tjh_batch_attr_set___Add_Attr_cmd(string $attr_name_textField,string $NodeName)
{

	string $attrValue_textField =  ` textField -q -text  ($attr_name_textField+"Value") `;
	string $attrName = ` textField -q -text  $attr_name_textField `;

	string $values[] ;
	tokenize $attrValue_textField " " $values;

	string $attrType = $values[0];

	//delete Attributes
	if(` objExists ($NodeName+"."+$attrName)` && $attrType == "Delete_Attr" )
	{					
		deleteAttr  ($NodeName+"."+$attrName);	
		print (("\n"+$NodeName+"."+$attrName)+ " has been Deleted OK ! ");
	
	}
	//else{warning ($NodeName+"."+$attrName+" don't Exist!");}


	//add Attributes
	if(!` objExists ($NodeName+"."+$attrName)`)
	{
		if($attrType == "Add_Vector")
		{		
			addAttr -ln $attrName  -at double3  $NodeName;
			addAttr -ln ($attrName+"X")  -at double -p $attrName  $NodeName;
			addAttr -ln ($attrName+"Y")  -at double -p $attrName  $NodeName;
			addAttr -ln ($attrName+"Z")  -at double -p $attrName  $NodeName;

			setAttr -type double3 ($NodeName+"."+$attrName) (float($values[1])) (float($values[2])) (float($values[3]));
			setAttr -e-keyable true ($NodeName+"."+$attrName);
			setAttr -e-keyable true ($NodeName+"."+$attrName+"X") ;
			setAttr -e-keyable true ($NodeName+"."+$attrName+"Y") ;
			setAttr -e-keyable true ($NodeName+"."+$attrName+"Z") ;
			
			//setAttr -type double3 ($NodeName+"."+$attrName) $values[1] $values[2] $values[3];
			setAttr ($NodeName+"."+$attrName+"X") (float($values[1])) ;
			setAttr ($NodeName+"."+$attrName+"Y") (float($values[2]));
			setAttr ($NodeName+"."+$attrName+"Z") (float($values[3]));
		}

		if($attrType == "Add_Int")
		{		
			addAttr -ln $attrName  -at long  -min (int($values[1])) -max (int($values[3])) -dv (int($values[2])) $NodeName;
			setAttr -e-keyable true ($NodeName+"."+$attrName);
		}
		if($attrType == "Add_String")
		{		
			addAttr -ln $attrName  -dt "string"  $NodeName;
			setAttr -e-keyable true ($NodeName+"."+$attrName);
			setAttr ($NodeName+"."+$attrName) -type "string" $values[1]   ;
		}
		if($attrType == "Add_Float")
		{		
			addAttr -ln $attrName  -at double   -min (float($values[1])) -max (float($values[3])) -dv (float($values[2])) $NodeName;
			setAttr -e-keyable true ($NodeName+"."+$attrName);
		}
		if($attrType == "Add_Boolean")
		{		
			addAttr -ln $attrName  -at bool  $NodeName;
			setAttr -e-keyable true ($NodeName+"."+$attrName);
		}
		if($attrType == "Add_Enum")
		{			
			string $enum ="";
			for($i=1;$i<size($values);$i++)
			{
				if($i==1)$enum+=$values[$i];
				if($i>1)$enum+=":"+$values[$i];
			}
			addAttr -ln $attrName  -at "enum" -en $enum   $NodeName;
			setAttr -e-keyable true ($NodeName+"."+$attrName);
		}

		print ("\n"+$NodeName+"."+$attrName+ " has been added OK . ");
	}else{warning ("\n"+$NodeName+"."+$attrName+" already Exist! please change name first !");}

}


