global proc tjh_batch_attr_set_Preset_management(string $method,string $presetName)

{ 
	if(size($presetName)==0){warning "preset is empty !"; }
	
	
	global string $tjh_batch_attr_set_Preset_datas[];
	if(size($tjh_batch_attr_set_Preset_datas)<5 )clear $tjh_batch_attr_set_Preset_datas;
	
	//for save tempUI data to memory.
	global string $tjh_batch_attr_set__tempUIStore[];

	global string $tjh_batch_attr_set_NAMES[];
		string $oldDatas[];
	$oldDatas = $tjh_batch_attr_set_Preset_datas;
	
	global int $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;
	
	
	if(size($presetName))
	{
		if($method == "load")///////////////////////////////
		{
				global string $tjh_batch_attr_set_Preset_datas[];		
				global string $tjh_batch_attr_set_NAMES[];

				global string $tjh_batch_attr_set__currentSelected_preset;
				 $tjh_batch_attr_set__currentSelected_preset = $presetName;
				
				global int $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;
				

				for($i = 0; $i<size($tjh_batch_attr_set_Preset_datas); $i++)
				{
					if($tjh_batch_attr_set_Preset_datas[$i] == "<preset>")
					{
						if($tjh_batch_attr_set_Preset_datas[$i+1] == $presetName)
						{
							
							
							$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num = int($tjh_batch_attr_set_Preset_datas[$i+2]);
							

							//update the main UI ******************

							tjh_batch_attr_set_mainUI;

							//*********************************

							int $newId = 1;

							//load the float int boolean enum  attrs
							for($k=0;$k<$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;$k++)
							{
								
								checkBox -e  -v (int($tjh_batch_attr_set_Preset_datas[$i+2+$newId])) ("tjh_batch_attr_set_checkBox_float"+($k+1)) ;
								
								$newId++;
								
								textField -e -text $tjh_batch_attr_set_Preset_datas[$i+2+$newId] ("tjh_batch_attr_set_textField_float"+($k+1)) ;
								
								$newId++;	

								textField -e -text $tjh_batch_attr_set_Preset_datas[$i+2+$newId] ("tjh_batch_attr_set_textField_float"+($k+1)+"Value") ;	

								$newId++;	
								
								string $cmd = "button -e -bgc  "+$tjh_batch_attr_set_Preset_datas[$i+2+$newId]+" tjh_batch_attr_set_button_colorGrap"+($k+1);
								eval $cmd;
								
								$newId++;
								
							}

							
						}
					}

				}//main for cycle end.
			
		}

		if($method == "insert_OneBlank_Attr_forCurrentPreset" ||$method == "remove_OneBlank_Attr_forCurrentPreset")///////////////////////////////
		{
				
					global string $tjh_batch_attr_set_Preset_datas[];		
					global string $tjh_batch_attr_set_NAMES[];
					

					global string $tjh_batch_attr_set__currentSelected_preset;
					 $tjh_batch_attr_set__currentSelected_preset = $presetName;
					
					global int $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;

					for($i = 0; $i<size($tjh_batch_attr_set_Preset_datas); $i++)
					{
						if($tjh_batch_attr_set_Preset_datas[$i] == "<preset>")
						{
							if($tjh_batch_attr_set_Preset_datas[$i+1] == $presetName)
							{
								
								int $tempNum = $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;

								//save current UI data to a temp memory .
								
								int $tempStoreId = 0;	
								clear $tjh_batch_attr_set__tempUIStore;

								for($k=0;$k<$tempNum;$k++)
								{
									if(` checkBox -ex ("tjh_batch_attr_set_checkBox_float"+($k+1))`)
									$tjh_batch_attr_set__tempUIStore[$tempStoreId]=` checkBox -q  -v  ("tjh_batch_attr_set_checkBox_float"+($k+1)) `;
									
									$tempStoreId++;
									
									if(` textField -ex ("tjh_batch_attr_set_textField_float"+($k+1))`)
									$tjh_batch_attr_set__tempUIStore[$tempStoreId]=` textField -q -text  ("tjh_batch_attr_set_textField_float"+($k+1)) `;
									
									$tempStoreId++;	

									if(` textField -ex ("tjh_batch_attr_set_textField_float"+($k+1)+"Value")`)
									$tjh_batch_attr_set__tempUIStore[$tempStoreId]=` textField -q -text  ("tjh_batch_attr_set_textField_float"+($k+1)+"Value") `;	

									$tempStoreId++;	
									
									if(` button -ex ("tjh_batch_attr_set_button_colorGrap"+($k+1))`)
									{
										float $color[] =` button -q -bgc  ("tjh_batch_attr_set_button_colorGrap"+($k+1)) `;
										$tjh_batch_attr_set__tempUIStore[$tempStoreId]=$color[0]+" "+$color[1]+" "+$color[2];	
									}
									$tempStoreId++;	
									
								}	
													

								
								//insert a blank attr.*******************************
								if($method == "insert_OneBlank_Attr_forCurrentPreset")$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num +=1;
								if($method == "remove_OneBlank_Attr_forCurrentPreset")$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num -=1;

								if($tjh_batch_attr_set__float_int_boolean_enumAttr_create__num < 0)
								$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num = 0;


								//update the main UI ******************

								tjh_batch_attr_set_mainUI;

								//*********************************


								//reload temp memory data for current UI.																			
								$tempStoreId = 0;

								for($k=0;$k<$tempNum;$k++)
								{
									if(` checkBox -ex ("tjh_batch_attr_set_checkBox_float"+($k+1))`)
									 checkBox -e  -v (int($tjh_batch_attr_set__tempUIStore[$tempStoreId]))  ("tjh_batch_attr_set_checkBox_float"+($k+1)) ;
									
									$tempStoreId++;
									
									if(` textField -ex ("tjh_batch_attr_set_textField_float"+($k+1))`)
									 textField -e -text $tjh_batch_attr_set__tempUIStore[$tempStoreId] ("tjh_batch_attr_set_textField_float"+($k+1)) ;
									
									$tempStoreId++;	

									if(` textField -ex ("tjh_batch_attr_set_textField_float"+($k+1)+"Value")`)
									 textField -e -text $tjh_batch_attr_set__tempUIStore[$tempStoreId] ("tjh_batch_attr_set_textField_float"+($k+1)+"Value") ;	

									$tempStoreId++;	
									
									if(` button -ex ("tjh_batch_attr_set_button_colorGrap"+($k+1))`)
									{
										string $cmd = "button -e -bgc  "+$tjh_batch_attr_set__tempUIStore[$tempStoreId]+" tjh_batch_attr_set_button_colorGrap"+($k+1);
										eval $cmd;
									}
									$tempStoreId++;	
									
								}
							}
						}

					}//main for cycle end.
				
		}

		


		if($method == "save")////////////////////////////////////
		{
			//check presetName exist or not
			for($i = 0; $i<size($oldDatas); $i++)
			{			

				if($oldDatas[$i] == "<preset>")
				{
					if($oldDatas[$i+1] == $presetName)
					{
						$presetName += "__<"+tjh_rand_Namer(6)+">" ;
					}
				}

			}//for end

			int $newId = size($tjh_batch_attr_set_Preset_datas);

			$tjh_batch_attr_set_Preset_datas[$newId] = "<preset>";
			$newId++;

			$tjh_batch_attr_set_Preset_datas[$newId] = $presetName;
			$newId++;
			
			$tjh_batch_attr_set_Preset_datas[$newId] = $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;
			$newId++;
				
			
			//save the float int boolean enum  attrs
			for($i=0;$i<$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;$i++)
			{
				
				int $toggle = ` checkBox -q  -v ("tjh_batch_attr_set_checkBox_float"+($i+1)) `;
				$tjh_batch_attr_set_Preset_datas[$newId] = $toggle;
				$newId++;
				
				string $attr = ` textField -q -text  ("tjh_batch_attr_set_textField_float"+($i+1)) `;
				$tjh_batch_attr_set_Preset_datas[$newId] = $attr;
				$newId++;	

				string  $floatValue= ` textField -q -text ("tjh_batch_attr_set_textField_float"+($i+1)+"Value") `;
				$tjh_batch_attr_set_Preset_datas[$newId] =  $floatValue;
				$newId++;


				float $color[] =` button -q -bgc  ("tjh_batch_attr_set_button_colorGrap"+($i+1)) `;
				$tjh_batch_attr_set_Preset_datas[$newId] = $color[0]+" "+$color[1]+" "+$color[2];			
				$newId++;
								
				
			}

			deleteUI tjh_batch_attr_set_PresetSaveWindow;		 
			tjh_batch_attr_set_Preset__saveDataToPresetFile_And_updateUI;

			//update the preset UI

			tjh_batch_attr_set_Preset;
			tjh_batch_attr_set_Preset_management "load" $presetName;
		
		}


		if($method == "delete")////////////////////////////////////
		{
			//global string $tjh_batch_attr_set_Preset_datas[];
			

			string $saveOldPresetDatas[] = $tjh_batch_attr_set_Preset_datas;		
			
			clear $tjh_batch_attr_set_Preset_datas;
			int $newId = 0;

			for($i = 0; $i<size($oldDatas); $i++)
			{			

				if($oldDatas[$i] == "<preset>" )
				{
					if($oldDatas[$i+1] == $presetName && $oldDatas[$i+1] != "<default>" )
					{
						int $floatAttrNum = int($oldDatas[$i+2]);					
						$i += 1+1 +$floatAttrNum*4 ; // save data number 
					}else{warning "<default> can not be remove ! ";}
				}

				$tjh_batch_attr_set_Preset_datas[$newId] = $oldDatas[$i];
				$newId++;
			}//for end		
		
			string $newPresetDatas[] =  $tjh_batch_attr_set_Preset_datas;
			
			//updata for menuItems preset
			tjh_batch_attr_set_Preset__saveDataToPresetFile_And_updateUI;


			//update the preset UI

			tjh_batch_attr_set_Preset;

			//load old datas from cache $saveOldPresetDatas and restore old UI datas display;		
			 clear $tjh_batch_attr_set_Preset_datas;
			 $tjh_batch_attr_set_Preset_datas = $saveOldPresetDatas;

			 tjh_batch_attr_set_Preset_management "load" $presetName;

			 //restore the new result datas
			clear $tjh_batch_attr_set_Preset_datas;
			$tjh_batch_attr_set_Preset_datas = $newPresetDatas;

			if($presetName == "<default>" ){warning "\n <default> can not be remove ! ";}

		}
        
	}else{warning "preset is empty !"; }
}//proc end



global proc string  tjh_rand_Namer(int $nameLength)
{
	string $dict[]={"a","b","c","d","e","f","g","h","i","j","k","l","m","n","o","p","q","r","s","t","u","v","w","x","y","z"};
	string $finalName= "";
	for($i=0;$i<$nameLength;$i++)
	{
		$finalName += $dict[int(rand(26))];	
	}

return $finalName;
} //tjh_rand_Namer(6);



global proc tjh_batch_attr_set__toggle_Invert()
{ 
	
	global int $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;
	
	//load the float int boolean enum  attrs
	for($k=0;$k<$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;$k++)
	{		
		int $toggle = ` checkBox -q  -v  ("tjh_batch_attr_set_checkBox_float"+($k+1)) `;
		if($toggle == 1) checkBox -e  -v 0 ("tjh_batch_attr_set_checkBox_float"+($k+1)) ;
		if($toggle == 0) checkBox -e  -v 1 ("tjh_batch_attr_set_checkBox_float"+($k+1)) ;
	
	}
	
}

global proc tjh_batch_attr_set__toggle_all_ON()
{ 	
	global int $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;	

	//load the float int boolean enum  attrs
	for($k=0;$k<$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;$k++)
	{		
		checkBox -e  -v 1 ("tjh_batch_attr_set_checkBox_float"+($k+1)) ;
	
	}

}

global proc tjh_batch_attr_set__toggle_all_Off()
{ 
	global int $tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;

	//load the float int boolean enum  attrs
	for($k=0;$k<$tjh_batch_attr_set__float_int_boolean_enumAttr_create__num;$k++)
	{		
		checkBox -e  -v 0 ("tjh_batch_attr_set_checkBox_float"+($k+1)) ;
	
	}
}

global proc tjh_batch_attr_set_openPreset()
{
	string $ScriptPath = `tjh_whatIs "tjh_batch_attr_set"`;	
	
	
	string $melFileName = $ScriptPath + "/tjh_batch_attr_set.preset.mel";

	system("notepad \""+$melFileName+"\"");

}







