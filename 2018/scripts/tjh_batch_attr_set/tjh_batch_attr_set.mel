global proc  tjh_batch_attr_set__removeStringNameWithDot(string $textField)
{
        string $text = ` textField -q -text $textField `;
        string $strings[];
        tokenize $text "." $strings;

        textField -e -text $strings[size($strings)-1] $textField;
}


global proc tjh_batch_attr_set__toggleUI_catcher_On()
{
	if(!` columnLayout -ex tjh_batch_attr_set__cmdScrollFieldReporter_columnLayout `)
	{
		columnLayout -p tjh_batch_attr_set__cmdScrollFieldReporter_frameLayout -w 400 -h 200 -en 0 tjh_batch_attr_set__cmdScrollFieldReporter_columnLayout;
		cmdScrollFieldReporter -p tjh_batch_attr_set__cmdScrollFieldReporter_columnLayout -width 400 -height 200 tjh_batch_attr_set__cmdScrollFieldReporter;
	}
}

global proc tjh_batch_attr_set__toggleUI_catcher_Off()
{
	if(` columnLayout -ex tjh_batch_attr_set__cmdScrollFieldReporter_columnLayout `)deleteUI  tjh_batch_attr_set__cmdScrollFieldReporter_columnLayout;
}


