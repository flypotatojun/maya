global proc tjh_batch_attr_set__advertisement(float $readAdsTimes)// unit: second.
{

		global int $tjh_batch_attr_set__advertisement___readTimes;
		//if($tjh_batch_attr_set__advertisement___readTimes==0)$tjh_batch_attr_set__advertisement___readTimes = 1;
		

		if($tjh_batch_attr_set__advertisement___readTimes < $readAdsTimes)
		{
			if (`windowPref -exists tjh_advertisement_window`)
				 windowPref -remove  tjh_advertisement_window;     

					  
			if (`window -exists tjh_advertisement_window`)
				 deleteUI tjh_advertisement_window;


			window -topLeftCorner 50 200    -titleBar 0 -w 585 -h 100 tjh_advertisement_window;

			columnLayout -adj 1 -cal "center";
				

				text -al "center" -l "\n\n\n 感谢您使用 tjh maya原创插件，如有问题，请反馈 tjh官方博客，祝您工作顺利，阿弥陀佛！\n\n\n" ;
				
				rowLayout -numberOfColumns 2
				    -columnWidth2 383 202
				    -adjustableColumn 2
				    
				    -columnAlign  1 "right" 
				    -columnAttach 1 "both"     0
				    -columnAttach 2 "both"  0;
				    
				   
				    
				    button -bgc .5 .7 1 -l "看最新、最全、tjh 全网首发原创插件，访问官方博客..." -c "showHelp -absolute \"http://blog.csdn.net/tian0000hai\"";
				    button -bgc .5 .5 1 -l "Close  关闭窗口" -c "deleteUI tjh_advertisement_window";



				    setParent ..; 

			showWindow;
		}

		$tjh_batch_attr_set__advertisement___readTimes+=1;

	//tjh_batch_attr_set__advertisementwaitTime $waitTime;

}



