		//text  -w 300   -al "center" -l ("   ");


		//refresh the list for textScrollList
		string $temp;

		//change the textScrollList constant.
		textScrollList -e -ra $tjh_batch_attr_set_PresetTextScrollList;//clear old list

		string $cmd="textScrollList -e "; 

		for($temp in $files){$cmd+=" -append \""+$temp+"\" ";};

		$cmd+= $tjh_batch_attr_set_PresetTextScrollList+";";
		$cmd+= "popupMenu -parent "+$tjh_batch_attr_set_PresetTextScrollList+";";
		$cmd+= "menuItem  -label \"Save...\" -c tjh_batch_attr_set_create_preset_window ;";
		$cmd+= "menuItem  -label \"Load\" -c tjh_batch_attr_set_read_preset ;";
		$cmd+= "menuItem  -label \"Delete\" -c tjh_batch_attr_set_delete_preset ;";

		eval $cmd;

		
		button -al "center" -bgc 0.1 0.5 0.5  -w 150 -ann "create Multi blank Attrs..."  -label "create Multi blank Attrs... "  -command  "tjh_batch_attr_set__cmd_setting";

		rowColumnLayout -numberOfRows 1;

			button  -bgc 0.1 0.3 0.5 -al "center"  -w 50 -ann "create preset"  -label "Save "  -command  "tjh_batch_attr_set_create_preset_window ";
			button  -bgc 0.1 0.3 0.4 -al "center"  -w 50 -ann "Load preset"  -label "Load "  -command  "tjh_batch_attr_set_read_preset";
			button  -bgc 0.1 0.3 0.3 -al "center"  -w 50 -ann "Delete preset"  -label "Delete"   -command  "tjh_batch_attr_set_delete_preset";


showWindow tjh_batch_attr_set_PresetWindow;

}


		    
global proc string[] tjh_batch_attr_set_Preset__getPresetNames()
{ 
	
	global string $tjh_batch_attr_set_Preset_datas[];	
	global string $tjh_batch_attr_set_Preset_NAMES[];

	string $presetNames[];
	int $id = 0;

	for($i = 0; $i<size($tjh_batch_attr_set_Preset_datas); $i++)
	{
		if($tjh_batch_attr_set_Preset_datas[$i] == "<preset>")
		{
			$presetNames[$id] =  $tjh_batch_attr_set_Preset_datas[$i+1];
			$id++;
		}

	}
	$tjh_batch_attr_set_Preset_NAMES = $presetNames;
return $presetNames;
}


global proc tjh_batch_attr_set_create_preset_window()
{
	global string $tjh_batch_attr_set_Preset_datas[];		
	
	if (`windowPref -exists tjh_batch_attr_set_PresetSaveWindow `)
		    windowPref -remove  tjh_batch_attr_set_PresetSaveWindow;     

			  
			  if (`window -exists tjh_batch_attr_set_PresetSaveWindow `)
			  deleteUI tjh_batch_attr_set_PresetSaveWindow;


	window  -menuBar true -rtf 1  -w 200 -h 50 tjh_batch_attr_set_PresetSaveWindow;

	columnLayout;	

	//separator -h 10 -w 200;

	text -w 200 -h 30 -al "left" -l "  Preset Name:";

	string $textField = ` textField -w 200  ` ;

	string $savecmd = "string $name =` textField -q -text  "+$textField+"`;tjh_batch_attr_set_Preset_management \"save\" $name ;";

	string $deletecmd = " deleteUI tjh_batch_attr_set_PresetSaveWindow;";

	separator -style "none" -h 10 -w 200;

	rowColumnLayout -numberOfColumns 2
			-columnWidth 1 100
			-columnWidth 2 100;
		
		button  -bgc 0.1 0.3 0.5 -l "save" -c $savecmd;
		button  -bgc 0.1 0.3 0.4 -l "Close" -c $deletecmd;
		setParent ..;
	showWindow;

}



global proc tjh_batch_attr_set_read_preset()

{
	global string $warning_select;

	global string $tjh_batch_attr_set_PresetTextScrollList;
	string $selectItem[]; $selectItem = ` textScrollList -q -si $tjh_batch_attr_set_PresetTextScrollList `;
	

	if($selectItem[0]!="")
	{
		tjh_batch_attr_set_Preset_management "load"  $selectItem[0];

	}else{warning $warning_select;};

}




global proc tjh_batch_attr_set_delete_preset()

{
	global string $warning_select;
	global string $tjh_batch_attr_set_PresetTextScrollList;
	string $selectItem[]; $selectItem = ` textScrollList -q -si $tjh_batch_attr_set_PresetTextScrollList `;

		
	if (`windowPref -exists tjh_batch_attr_set_PresetDeleteWindow `)
		    windowPref -remove  tjh_batch_attr_set_PresetDeleteWindow;     

			  
			  if (`window -exists tjh_batch_attr_set_PresetDeleteWindow `)
			  deleteUI tjh_batch_attr_set_PresetDeleteWindow;


	window  -menuBar true -rtf 1  -w 230 -h 50 tjh_batch_attr_set_PresetDeleteWindow;

		columnLayout;	

		//separator -h 10 -w 200;

		text -w 230 -h 30 -al "left" -l "   Are you sure to delete the current preset !!!";		

		string $delCmd = "tjh_batch_attr_set_Preset_management \"delete\" \""+$selectItem[0]+"\";";
		string $deletecmdUI = " deleteUI tjh_batch_attr_set_PresetDeleteWindow";

		separator -style "none" -h 10 -w 230;

		rowColumnLayout -numberOfColumns 2
				-columnWidth 1 100
				-columnWidth 2 100;
			
			button -bgc 0.1 0.3 0.5 -l "Yes" -c ($delCmd+$deletecmdUI);
			button -bgc 0.1 0.3 0.4 -l "No" -c $deletecmdUI;
			setParent ..;
	showWindow;

		
}





