global proc tjh_batch_attr_set_Preset__saveDataToPresetFile_And_updateUI()
{ 

	global string  $tjh_batch_attr_set_Preset_datas[];		
	
	string $ScriptPath = `tjh_whatIs "tjh_batch_attr_set"`;	

	
	string $melFileName = $ScriptPath + "/tjh_batch_attr_set.preset.mel";

        if(` filetest -e $melFileName `)//read the datas from file
	{
		int    $fileDesId=` fopen $melFileName "a"`;
		
		int $size = size($tjh_batch_attr_set_Preset_datas);
		

		fprint $fileDesId "global string  $tjh_batch_attr_set_Preset_datas[];$tjh_batch_attr_set_Preset_datas={";
		//if($size == 1)$size = 0;
		if( $size >10)
		{
			for($i=0;$i<$size;$i++)
			{
				if($i<($size-1)) fprint $fileDesId ("\""+$tjh_batch_attr_set_Preset_datas[$i]+"\",");
				if($i==($size-1)) fprint $fileDesId ("\""+$tjh_batch_attr_set_Preset_datas[$i]+"\"");
			}
		}else{
			clear $tjh_batch_attr_set_Preset_datas;
		}

		fprint $fileDesId "};";

		fclose $fileDesId;

	//updata UI.
	tjh_batch_attr_set_mainUI;
	global string $tjh_batch_attr_set__currentSelected_preset;
	tjh_batch_attr_set_Preset_management "load"  $tjh_batch_attr_set__currentSelected_preset;
        
        }else{

        tjh_batch_attr_set_read_preset_initial_readDataFromPresetFile;
        
        }


        print "\n Save The Out Data To Preset File For Initial Ok  !";
}

global proc string tjh_whatIs(string $mel)
{
	string $path = ` whatIs $mel `;
	//转换 "\" 路径为 "/" 路径
	$path= dirname($path)+"/"+$mel;
	string $temp1[];
 	$num = `tokenize $path ":" $temp1`;
	$path = strip ($temp1[1]+":"+$temp1[2]);

	string $temp2[];
 	$num = `tokenize $path "/" $temp2`;
	$path = $temp2[0];

	for ( $i = 1; $i<( $num-1); $i++ )
	{
		$path = $path + "/" + $temp2[$i];		
	}
return	$path;	
}


