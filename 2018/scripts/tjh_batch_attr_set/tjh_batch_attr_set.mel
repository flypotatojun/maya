global proc tjh_batch_attr_set__select(string $method)
{
	
	string $selects[];
	int $HierarchyToggle = ` checkBox -q -v tjh_batch_attr_set__checkBox__HierarchyToggle `;
	int $SelectedToggle = ` checkBox -q -v  tjh_batch_attr_set__checkBox__SelectedToggle `;
	//int $SelectedToggle = 0;
	
	if($method =="nodetype")
	{
		string $nodetype = ` textField -q -text tjh_batch_attr_set__textField__nodeType_to_selected `;
		if(size($nodetype)>0 && $nodetype != "(Node Type: mesh)")
		{
			
			if($HierarchyToggle==0 && $SelectedToggle == 0) select -r -ne ` ls -type $nodetype `;
			if($HierarchyToggle==1 && $SelectedToggle == 0) select -r -ne ` ls -dag  -type $nodetype `;
			if($HierarchyToggle==0 || $HierarchyToggle==1  && $SelectedToggle == 1 ) 			
			{
				string $selTemp[];$selTemp = ` listConnections -type $nodetype `;
				if(size($selTemp)==0){	select -r -ne ` ls  -sl  -dag  -type  $nodetype `;}//shape node type :mesh nurbsSurface
				if(size($selTemp)>0){	select -r -ne ` listConnections -type $nodetype `;}//cluster 
			}
			
			
			//if($HierarchyToggle==0 && $SelectedToggle == 0)select -r ` ls -type $nodetype `;
			//if($HierarchyToggle==0 && $SelectedToggle == 1)select -r  ` ls -sl -type $nodetype  `;
			//if($HierarchyToggle==1 && $SelectedToggle == 0)select -r  ` ls -dag -type $nodetype `;
			//if($HierarchyToggle==1 && $SelectedToggle == 1)select -r  ` ls -dag -sl -type $nodetype  `;
			
			 
		}else{warning ("\n  node type textField is not set value,please set first !");return;}
	}

	if($method =="name")
	{
		string $name = ` textField -q -text tjh_batch_attr_set__textField__name_to_selected `;
		if(size($name)>0 && $name != "(Name: abc)")
		{
			if($HierarchyToggle==0 && $SelectedToggle == 0)select -r -ne ` ls ("*"+ $name +"*") `;
			if($HierarchyToggle==0 && $SelectedToggle == 1)select -r -ne ` ls -sl ("*"+ $name +"*")  `;
			if($HierarchyToggle==1 && $SelectedToggle == 0)select -r -ne ` ls -dag ("*"+ $name +"*") `;
			if($HierarchyToggle==1 && $SelectedToggle == 1)select -r -ne ` ls -dag -sl ("*"+ $name +"*")  `;
			

		}else{warning ("\n  name textField is not set value,please set first !");return;}
	}
}

global proc  tjh_batch_attr_set___runSingleAttr(string $attrTextFieldUI,string $attrValueTextFieldUI)
{

	string $selects[];$selects = ` ls -sl `;
	string $sel;
	string $attr = ` textField -q -text  $attrTextFieldUI `;
	string $floatValue = ` textField -q -text  $attrValueTextFieldUI `;
	string $attrArrays[];
	string $valueArrays[];
	tokenize $attr "." $attrArrays;
	tokenize $floatValue " " $valueArrays;

	if(size($selects)>0)
	{	
		
		for($sel in $selects)//main for cycle
		{
			//add custom attibutes for selected nodes
			if($valueArrays[0]=="Delete_Attr"||$valueArrays[0]=="Add_Vector"||$valueArrays[0]=="Add_Int"||$valueArrays[0]=="Add_String"||$valueArrays[0]=="Add_Float"||$valueArrays[0]=="Add_Boolean"||$valueArrays[0]=="Add_Enum")
			{
				tjh_batch_attr_set___Add_Attr_cmd  $attrTextFieldUI  $sel;
			}else{

				
				
				if(size($attrArrays)== 1)//for obj.attr setting
				{	
					if ( ` objExists  ($sel+"."+$attr) `)
					{					
						if($floatValue  != ""  )
						{
							
							string $attrType = ` getAttr -type ($sel+"."+$attr)`;
							string $cmd="";

							if($attrType == "string")
							{
								$cmd="setAttr  -type \"string\" "+$sel+"."+$attr+" \""+$floatValue+"\";";
							}else if($attrType == "long2" ||$attrType == "long3" ||$attrType == "short2" ||$attrType == "short3" ||$attrType == "float2" ||$attrType == "float3" || $attrType == "double3" || $attrType == "double2")
							{	
								string $floatToStringValue = "";
								for($j = 0 ;$j<size($valueArrays);$j++)
								{
									if($j==0)$floatToStringValue +=$valueArrays[$j];
									if($j>0)$floatToStringValue +=" "+$valueArrays[$j];
								}
								$cmd="setAttr "+$sel+"."+$attr+" -type "+$attrType+" "+$floatToStringValue+" ;";
							}else {
								$cmd="setAttr "+$sel+"."+$attr+" "+$floatValue+";";
							}

							eval $cmd;
							print ("\n set "+$sel+"."+$attr+" attribute ok !");

						}else{warning ("\n  float type: "+$attr+" is not set value,please set first !");break;}
					}

					
				}else if(size($attrArrays)>= 2)//for global attr setting
				{
					if (` objExists  $attr `)
					{
						
						string $attrType = ` getAttr -type $attr `;
						string $cmd="";
						
						if($floatValue  != "")
						{

							if($attrType == "string")
							{
								$cmd="setAttr  -type \"string\" "+$attr+" \""+$floatValue+"\";";
							}else if($attrType == "long2" ||$attrType == "long3" ||$attrType == "short2" ||$attrType == "short3" ||$attrType == "float2" ||$attrType == "float3" || $attrType == "double3" || $attrType == "double2")
							{	
								string $floatToStringValue = "";
								for($j = 0 ;$j<size($valueArrays);$j++)
								{
									if($j==0)$floatToStringValue +=$valueArrays[$j];
									if($j>0)$floatToStringValue +=" "+$valueArrays[$j];
								}
								$cmd="setAttr "+$attr+" -type "+$attrType+" "+$floatToStringValue+" ;";
							}else {
								$cmd="setAttr "+$attr+" "+$floatValue+";";
							}
							eval $cmd;						
							print ("\n set "+$attr+" attribute ok !");

						}else{warning ("\n  float type: "+$attr+" is not set value,please set first !");}
					}
				//break;

				}
			}
				
		}//for selected end
	
	
	
	
	}else if (size($selects)==0 ) // if nothing selected  and set the full name attribute with dot "."
	{

		if ($attr != "" && ` objExists  $attr `)
		{
			
			string $attrType = ` getAttr -type $attr `;
			string $cmd="";
			
			if($floatValue  != "")
			{

				if($attrType == "string")
				{
					$cmd="setAttr  -type \"string\" "+$attr+" \""+$floatValue+"\";";
				}else if($attrType == "long2" ||$attrType == "long3" ||$attrType == "short2" ||$attrType == "short3" ||$attrType == "float2" ||$attrType == "float3" || $attrType == "double3" || $attrType == "double2")
				{	
					string $floatToStringValue = "";
					for($j = 0 ;$j<size($valueArrays);$j++)
					{
						if($j==0)$floatToStringValue +=$valueArrays[$j];
						if($j>0)$floatToStringValue +=" "+$valueArrays[$j];
					}
					$cmd="setAttr "+$attr+" -type "+$attrType+" "+$floatToStringValue+" ;";
				}else {
					$cmd="setAttr "+$attr+" "+$floatValue+";";
				}
				eval $cmd;						
				print ("\n set "+$attr+" attribute ok !");

			}else{warning ("\n  float type: "+$attr+" is not set value,please set first !");}
		}			

	} //end else if 
	//print "\nset attribute ok !";

}


global proc tjh_batch_attr_set__run(string $method) // "selected" | "nodetype" |"name"
