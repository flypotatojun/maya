	if (` windowPref -exists tjh_batch_attr_set_PresetWindow `)
            windowPref -remove  tjh_batch_attr_set_PresetWindow;     

                  
        if (` window -exists tjh_batch_attr_set_PresetWindow `)
       deleteUI tjh_batch_attr_set_PresetWindow;


	window 
		//-bgc 0.276 0.307 0.426	
		-rtf 1 -w 150 -h 200  
		tjh_batch_attr_set_PresetWindow;

	columnLayout -columnAlign "center";

		text  -w 150  -fn "boldLabelFont"  -al "center" -l ("\n  Preset Management \n");
		text -ww 1 -w 150   -al "center" -l " double click to load preset.  ";

