global proc tjh_batch_attr_set__advertisementwaitTime(float $Seconds)
{
    float $startTime = `timerX`;
    float $elaspedTime = 0;
    while ($elaspedTime < $Seconds ) {  $elaspedTime = `timerX -startTime $startTime`;  }
    print ("Wait Time: "+$elaspedTime+"\n");
}

